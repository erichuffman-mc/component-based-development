{
  "name": "nitflex_theme",
  "version": "1.0.0",
  "scripts": {
    "build": "patternlab build --config ./patternlab-config.json",
    "compile": "gulp compile",
    "watch": "gulp watch",
    "styleguide": "gulp styleguide",
    "compress": "gulp compress",
    "lint": "gulp lint",
    "clean": "gulp clean",
    "gulp": "gulp",
    "patternlab": "patternlab",
    "help": "patternlab --help",
    "install": "patternlab install --config ./patternlab-config.json",
    "serve": "patternlab serve --config ./patternlab-config.json",
    "start": "npm run serve",
    "version": "patternlab --version",
    "export": "patternlab export"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "IE >= 9",
              "last 3 versions"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "transform-remove-strict-mode"
    ]
  },
  "browserslist": [
    "IE >= 9",
    "last 3 versions"
  ],
  "devDependencies": {
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "babel-preset-env": "^1.6.0",
    "breakpoint-sass": "^2.7.1",
    "browser-sync": "^2.18.12",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^4.0.0",
    "gulp-imagemin": "^3.3.0",
    "gulp-order": "^1.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sass-lint": "^1.3.2",
    "gulp-shell": "^0.6.5",
    "gulp-sourcemaps": "^2.6.0",
    "run-sequence": "^2.0.0",
    "@pattern-lab/cli": "0.0.3-alpha.0",
    "@pattern-lab/patternlab-node": "^3.0.0-alpha.8"
  },
  "dependencies": {
    "@pattern-lab/cli": "^0.0.3-alpha.0",
    "@pattern-lab/core": "^3.0.1-alpha.0",
    "@pattern-lab/engine-twig-php": "^3.0.0",
    "@pattern-lab/starterkit-twig-demo": "^4.1.1",
    "@pattern-lab/uikit-workshop": "^1.0.1-alpha.0"
  }
}
